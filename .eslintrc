{
  "plugins": ["prettier"],
  "extends": ["airbnb-base", "plugin:prettier/recommended", "prettier"],
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "no-debugger": "off",
    "no-console": 0,
    "no-unused-vars": 1,
    "no-useless-escape": 1
  }
}
